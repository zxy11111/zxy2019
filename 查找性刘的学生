#include<stdio.h>
#include<stdlib.h>
#include<string.h>
struct student{
	char name[20];
}s[100],s1[100];
int lookup(student a[100],int n,student b[100])
{
	int i,ans=0;
	for(i=0;i<n;i++)
	{
		if(a[i].name[0]=='åˆ˜')
		{
			ans++;
			strcpy(b[i].name,a[i].name);
		}
	}
	return ans;
}
void main()
{
	int n;
	while(scanf("%d",&n)!=EOF)
	{
		int i;
		for(i=0;i<n;i++)
		{
			scanf("%s",s[i].name);
		}
		int c=lookup(s,n,s1);
		printf("%d",c);
		system("pause");
	}
}
